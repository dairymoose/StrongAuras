<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
	<Script file="StrongAuras.lua"/>
	<Frame name="StrongAuras_Frame" parent="UIParent" hidden="true">
		<Scripts>
			<OnLoad>
				StrongAuras_OnLoad()
			</OnLoad>
			<OnEvent>
				StrongAuras_OnEvent()
			</OnEvent>
			<OnUpdate>
				StrongAuras_OnUpdate(arg1)
			</OnUpdate>
			<OnDragStart>
				self:StartMoving()
			</OnDragStart>
			<OnDragStop>
				self:StopMovingOrSizing()
			</OnDragStop>
		</Scripts>

		<Size>
			<AbsDimension x="200" y="10" />
		</Size>

		<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background">
		</Backdrop>

		<Layers>
			<Layer level="ARTWORK">
				<Texture name="StrongAuras_mainhand">
					<Size>
						<AbsDimension x="10" y="10" />
					</Size>
					<Anchors>
						<Anchor point="LEFT">
						</Anchor>
					</Anchors>
				</Texture>
				<Texture name="StrongAuras_FrameTime">
					<Size>
						<AbsDimension x="200" y="10" />
					</Size>
					<Anchors>
						<Anchor point="CENTER">
						</Anchor>
					</Anchors>
				</Texture>
				<FontString name="StrongAuras_maintimer" justifyH="RIGHT" justifyV="MIDDLE">
				</FontString>
			</Layer>
		</Layers>
	</Frame>
</Ui>
